  },
  "homepage": "https://github.com/cyss0317/ToDoList#readme",
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "body-parser": "^1.19.0",
    "concurrently": "^6.3.0",
    "express": "^4.17.1",
    "mongoose": "^6.0.12",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.14"
  }
